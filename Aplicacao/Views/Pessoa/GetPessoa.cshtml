@{ ViewData["Title"] = "Alterar Pessoa"; }

@section Styles {
    <link rel="stylesheet" href="~/lib/jquery-ui/jquery-ui.min.css">
    <link rel="stylesheet" href="~/lib/jquery-ui/jquery.timepicker.min.css">
}

@section Scripts {
    <script>
        var id = @(ViewBag.Id);
    </script>
    <script src="~/js/PessoaExpandir.js"></script>
    <script src="~/lib/jquery-mask/jquery-mask.min.js"></script>
}

<h1>Detalhes da Pessoa</h1>

<div class="row">
    <div class="col-md-6">
        <div id="form-telefone">
            <div class="form-group">
                <label for="nome">Nome</label>
                <input type="text" class="form-control" id="nome" disabled>
            </div>
            <div class="form-group">
                <label for="dataNascimento">Data de Nascimento</label>
                <input type="text" class="form-control" id="dataNascimento" disabled>
            </div>
               
        </div>      
    </div>
</div>
